<?xml version="1.0"?>
<sdf version="1.6">
  <world name="default">
    <include>
      <uri>file:///home/ironman/software/gazebo_models/ground_plane</uri>
    </include>

     <include>
      <uri>file:///home/ironman/software/gazebo_models/sun</uri>
    </include>

    <model name="box">
      <link name="link">
        <pose>0 0 0.5 0 0 0</pose>

        <collision name="box_collision">
          <geometry>
            <box>
              <size>1 1 1</size>
            </box>
          </geometry>
        </collision>

        <visual name="visual">
          <geometry>
            <box>
              <size>1 1 1</size>
            </box>
          </geometry>
        </visual>

        <sensor name='my_camera' type='camera'>
          <plugin name="gazebo_robocomp_camera" filename="libgazebo_robocomp_camera.so"/>
          <contact>
            <collision>box_collision</collision>
          </contact>
        </sensor>
      </link>
    </model>
  </world>
</sdf>
